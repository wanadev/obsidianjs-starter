{
  "name": "starter",
  "version": "0.0.0",
  "description": "Obsidian Starter Kit",
  "main": "src/index.js",
  "repository": "git@github.com:wanadev/obsidianjs-starter.git",
  "author": "wanadev",
  "license": "MIT",
  "scripts": {
    "start": "node server",
    "dev": "cross-env NODE_ENV=dev NODE_OPTIONS=--openssl-legacy-provider node server",
    "release": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider webpack --config ./config/webpack.config.release.js && cp -r server build/release && cp package.json build/release && cd build/release && npm install --only=prod --ignore-scripts",
    "lint": "eslint src config server",
    "lint-fix": "eslint src config server --fix",
    "test": "echo \"tests: todo !\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.19.4",
    "@obsidianjs/obsidian": "^0.3.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "license-webpack-plugin": "^4.0.2",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-loader": "^7.0.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-glsl-loader": "^1.0.1",
    "webpack-hot-middleware": "^2.25.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
